#: src/getopt.c:691
#, c-format
msgid "%s: option `%s' is ambiguous\n"
msgstr "%s: flagget «%s» er fleirtydig\n"

#: src/getopt.c:716
#, c-format
msgid "%s: option `--%s' doesn't allow an argument\n"
msgstr "%s: flagget «--%s» tek ikkje argument\n"

#: src/getopt.c:721
#, c-format
